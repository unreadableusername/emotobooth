{
  "name": "emotobooth",
  "version": "2.0.0",
  "description": "Use the Google Vision API's Face Detection to apply beautiful treatments to photos.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:server": "nodemon --watch ./ --inspect=0.0.0.0:9229 server.js",
    "watch:ui": "webpack --watch",
    "watch": "concurrently --kill-others -p \"[{name}]\" -n \"SERVER,UI\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run watch:server\" \"npm run watch:webpack\"",
    "watch:webpack": "webpack --watch",
    "start": "node server.js",
    "build": "webpack"
  },
  "nodemonConfig": {
    "ignore": [
      "build/*",
      "ui/*",
      "in/*",
      "out/*",
      "out-debug/*",
      "out-photostrips/*",
      "out-print/*",
      "package.json"
    ]
  },
  "author": "Instrument",
  "repository": {
    "type": "git",
    "url": "git://github.com/Instrument/emotobooth-image-processing"
  },
  "license": "ISC",
  "readme": "./README.md",
  "dependencies": {
    "bluebird": "3.3.1",
    "canvas": "1.3.12",
    "chokidar": "1.4.2",
    "express": "4.13.4",
    "fs": "0.0.2",
    "github": "0.2.4",
    "gm": "1.21.1",
    "gsap": "1.18.2",
    "hashish": "0.0.4",
    "http-server": "0.8.5",
    "kue": "0.10.5",
    "minimist": "1.2.0",
    "node-uuid": "1.4.7",
    "phantomjs-prebuilt": "2.1.5",
    "pixi.js": "3.0.11",
    "redis": "2.8.0",
    "request": "2.69.0",
    "socket.io": "1.4.5",
    "socket.io-client": "1.4.5",
    "sprintf": "0.1.5",
    "style-loader": "0.13.0",
    "temp": "0.8.3",
    "twitter": "1.2.5",
    "underscore": "1.8.3",
    "winston": "2.2.0",
    "yargs": "4.3.1"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.6.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "brfs": "1.4.3",
    "concurrently": "^3.5.1",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "1.3.0",
    "json-loader": "0.5.4",
    "node-sass": "3.7.0",
    "nodemon": "^1.15.0",
    "path": "0.12.7",
    "sass-loader": "3.1.2",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "webpack-livereload-plugin": "0.5.3"
  }
}
